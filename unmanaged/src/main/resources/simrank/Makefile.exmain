#
## Makefile for non-Microsoft compilers
#

VERSION = -std=c++11 -Wall

.PHONY: all
all: $(SIMRANK)

# other
FingerPrint.o: FingerPrint.cpp FingerPrint.h
	$(CC) -c $(VERSION)  $(CXXFLAGS) $(CFLAGS)   FingerPrint.cpp -o FingerPrint.o  -I$(INC1) -I$(INC2) -I$(INC3) -I$(GL) -I$(ARM)

