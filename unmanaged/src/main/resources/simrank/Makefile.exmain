	$(CC) -c $(VERSION)  $(CXXFLAGS) $(CFLAGS)   OptSimMatSVD.cpp -o OptSimMatSVD.o  -I$(INC1) -I$(INC2) -I$(INC3) -I$(GL) -I$(ARM)

