	$(CC) -c $(VERSION)  $(CXXFLAGS) $(CFLAGS)    naiveSR.cpp -o naiveSR.o  -I$(INC1) -I$(INC2) -I$(INC3) -I$(GL) -I$(ARM)

OIPSimRank.o: OIPSimRank.cpp OIPSimRank.h
	$(CC) -c  $(VERSION) $(CXXFLAGS) $(CFLAGS)   OIPSimRank.cpp -o OIPSimRank.o  -I$(INC1) -I$(INC2) -I$(INC3) -I$(GL) -I$(ARM)

