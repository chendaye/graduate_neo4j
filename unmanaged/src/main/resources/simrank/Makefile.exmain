	$(CC) -c $(VERSION)  $(CXXFLAGS) $(CFLAGS)   KMSRIndex.cpp -o KMSRIndex.o  -I$(INC1) -I$(INC2) -I$(INC3) -I$(GL) -I$(ARM)


